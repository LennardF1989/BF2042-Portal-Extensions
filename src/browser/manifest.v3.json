{
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "128": "icon-128.png"
        },
        "default_popup": "options/index.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "web/app.js"
            ],
            "matches": [
                "https://portal.battlefield.com/*"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}